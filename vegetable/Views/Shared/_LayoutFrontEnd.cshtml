    <style>
        body {
            font-family: 'Open Sans', sans-serif;
            font-size: 11px;
            font-weight: 600;
            letter-spacing: 2px;
        }

        .icon {
            font-size: 18px;
        }

        .band {
            
            color: black;
        }
        .band:hover {
             text-decoration:none;
             color:black;
        }
        .navTop {
            line-height: 40px;
        }
        .title {
            font-family: 'Merriweather', serif;
            font-size: 55pt;
            font-weight: 400;
        }

        .btn-outline-dark {
            color: #343a40;
            border-color: #343a40;
            border: 3px solid;
            background: white;
        }

        .dot:hover {
            border-bottom: 1px dotted;
        }

        .icon:hover {
            color: black;
            transition: .2s;
        }
/*頁面商品分類選單*/
         .main-nav{
             display:none;
         }
         .main-nav .nav-item .nav-link .line {
            border-bottom: 2px solid white;
            display: block;
            transition: 0.5s;
        }

        .main-nav .nav-item:hover .nav-link .line {
            transition: 0.5s;
            border-bottom: 2px solid black;
        }

        .menu-block {
            background-color: rgba(250,250,250,.95);
            width: 100%;
            position: absolute;
            left: 0;
            margin-top: 10px;
            box-shadow: 1px 1px 10px #ddd;
            padding: 40px;
            z-index: 99;
            display: none;
            opacity: 0;
        }
        .bg-menu {
             background-color: rgba(250,250,250,.95);
             letter-spacing:0.5px;
             font-weight:100;
        }
        .menu-list{
            padding:0;
         }
        .menu-list li {
            padding:.3rem 0;
            list-style:none;
            color:black
        }
        .menu-list li a,
        .menu-list li a:hover{
             text-decoration:none;
             color:black;
        }
        .menu-list li:not(:first-child){
            font-weight:100;
        }
        .main-nav .nav-item:hover .menu-block {
            display: block;
            transition: .2s;
            opacity: 1;
        }
 /*search-bar相關 mobile*/        
        .search-bar-sm{
            width:90%;
            margin:1rem auto;
            border-bottom:2px solid black;
             color: black;
        }
            .search-bar-sm input {
                margin-top: 0;
                border: none;
                font-size: 12px;
                
            }
                .search-bar-sm input::placeholder { /* Chrome, Firefox, Opera, Safari 10.1+ */
                    color: black;
                    opacity: 1; /* Firefox */
                    transition:.2s linear;
                }

                .search-bar-sm input:-ms-input-placeholder { /* Internet Explorer 10-11 */
                    color: black;
                }

                .search-bar-sm input::-ms-input-placeholder { /* Microsoft Edge */
                    color: black;
                }

                .search-bar-sm input:focus::placeholder { /* Chrome, Firefox, Opera, Safari 10.1+ */
                    padding-left:20px;
                    opacity: 0; /* Firefox */
                    transition:.2s linear;
                }

                .search-bar-sm input:focus:-ms-input-placeholder { /* Internet Explorer 10-11 */
                    padding-left:20px;
                    opacity: 0; /* Firefox */
                    transition:.2s linear;
                }

                .search-bar-sm input:focus::-ms-input-placeholder { /* Microsoft Edge */
                    padding-left:20px;
                    opacity: 0; /* Firefox */
                    transition:.2s linear;
                }
         .search-bar-sm .input-group-text{
             background:none;
             border:none;
         }
          .search-bar-sm input:focus{
              box-shadow:none;
          }
          @@media screen and (min-width:992px){
             .search-bar-sm{
                display:none;
            }
            .main-nav{
                display:flex;
            }
          
          }
 /*footer相關*/
        .bg-footer {
                background: #d4a299;
        }
        .footericon {
            font-size: 26px;
        }
        .footertext {
            font-weight: 400;
        }
        .footer-list{
            padding:0;
            margin-top: 3rem;
         }
        .footer-list li  {
            padding:.5rem .10rem;
            list-style:none;
            margin-right: 12px;
            letter-spacing:1px;
            color:white;
        }
         .footer-list li a,
         .footer-list li a:hover{
           color:white;
           text-decoration:none;
         }
        .footer-list li:not(:first-child){
            font-weight:100;
        }
/*Login modal*/
        input {
            padding: 12px 20px;
            margin: 8px 0;
            display: inline-block;
            border: 1px solid #ccc;
            box-sizing: border-box;
        }

        button {
            padding: 14px 20px;
            cursor: pointer;
            font-size: 12px;
            font-weight: 900;
            letter-spacing: 2px;
        }

            button:hover {
                opacity: 0.8;
            }

        .closecontainer {
            text-align: center;
            margin: 24px 0;
            position: relative;
        }

        .modal {
            display: none;
            position: fixed;
            z-index: 1;
            left: 0;
            top: 0;
            width: 100%;
            height: 100%;
            overflow: auto;
            background-color: rgb(0, 0, 0);
            background-color: rgba(0, 0, 0, 0.4);
            padding-top: 60px;
        }

            .modal label {
                padding: 10px 0;
                margin-top: 5px;
                margin-bottom: 0;
            }

            .modal input {
                padding: 10px 0;
                margin-top: 5px;
                margin-bottom: 0;
            }

            .modal button {
                padding: 10px 0;
                margin: 20px 0;
            }

        .modal-content {
            background-color: #fefefe;
            margin: 0% auto 10% auto;
            border: 1px solid #888;
            width: 550px;
            height: 500px;
            overflow: auto;
        }

        .close {
            position: absolute;
            right: 25px;
            top: 0;
            color: #e82b1a;
            font-size: 25px;
            font-weight: bold;
        }

            .close:hover,
            .close:focus {
                color: #e82b1a;
                cursor: pointer;
            }

        .animate {
            -webkit-animation: animatezoom 0.6s;
            animation: animatezoom 0.6s
        }
    </style>
</head>
