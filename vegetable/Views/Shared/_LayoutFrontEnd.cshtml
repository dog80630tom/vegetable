                        <img src="~/Content/images/discover-logo-of-pay-system.png" alt="Alternate Text" />
                    </a>
                </div>
            </div>
        </div>
    </footer>


    <div class="modal fade" id="loginModal" style="z-index:1000000000">
        <div class="modal-dialog">
            <div class="modal-content animate  rounded-0 m-0">
                <button type="button" class="close mt-3" data-dismiss="modal">&times;</button>
                <div class="container mt-5">
                    <div class="row px-5">
                        <div class="col-8 mx-auto justify-content-center">
                            <h4><b>Sign In</b></h4>
                            <label for="uname"><b>Email Address<span class="text-danger">*</span></b></label>
                            <input class="w-100" type="text" placeholder="Enter Email" name="uname" id="loginId" required>
                            <label for="psw"><b>Password<span class="text-danger">*</span></b></label>
                            <input class="w-100" type="password" placeholder="Enter Password" name="psw" id="loginPsw" required>
                            <button class="btn-outline-dark w-100 btn-block" type="submit" id="loginConfirm">LOGIN</button>
                            <button class="btn-outline-dark w-100 btn-block" id="gotoRegist">REGISTER</button>
                            <label class="mt-1 pt-0" for="pswproblem"><b>Forget your Password?</b></label><br />
                            <label class="mt-1 pt-0" for="pswproblem" id="loginStatus" style="color:red"></label>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>

        @RenderSection("viewJquery", required: false)
        @RenderSection("Endscripts", required: false)

        <script>
            $(function () {
                $("#btn_login").click(function () {
                    $('#loginModal').modal('show');
                });
                $("#gotoRegist").click(function () {
                    location.href="/FrontEnd/MemberRegist"
                });

                    var uname = $("#loginId").val();
                    var psw = $("#loginPsw").val();
                    console.log('uname is', uname);
                    console.log('psw is', psw);

                    $.ajax({
                        type: "POST",
                        url: "/Members/Login",
                        data: { uname, psw },
                        success: function (back) {
                            console.log('back', back);
                            if (back == "1") {
                                $("#loginStatus").html("登入成功");
                            }
                            else if (back == "2") {
                                $("#loginStatus").html("此帳號尚未註冊，請先註冊");
                            }
                            else {
                                $("#loginStatus").html("密碼錯誤");
                            }

                        }

                    });

                });     
    </script>
  @RenderSection("Endscripts", required: false)
</body>
</html>

