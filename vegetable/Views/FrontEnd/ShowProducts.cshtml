        .prod-item .prod-image {
            position:relative;
            margin-bottom:21px;
            display:flex;
            flex-wrap:wrap;
            justify-content:center;
        }

        .prod-item .prod-image > a {
            display:inline-flex;
            justify-content:center;
            align-items:center;
            align-content:center;
            width:100%;
